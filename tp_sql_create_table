
/*
drop table ING_PRO_CPC
drop table PRO_CPC
drop table ING_CPC
drop table AFFECT
drop table EMPLOI
drop table CLI_PRO
drop table CLIENT
drop table PROJET
drop table TYPE
drop table INGENIEUR
drop table DEPT
*/
create table TYPE
(
TYPE     smallint     NOT NULL,
LIBTYPE  varchar (20) NOT NULL,
constraint PK_TYPE primary key (TYPE)
)


create table INGENIEUR
(
MLE       int          NOT NULL,
NOM       char (20)    NOT NULL,
PRENOM    char (20)    NOT NULL,
NOSS      char (13)    NOT NULL,
DATNAIS   datetime             ,
NOTEL     char (14)            ,
ADRESSE   varchar (30) NOT NULL,
CODEPOST  char (5)     NOT NULL,
VILLE     char (30)    NOT NULL,
SEXE      char (1)     NOT NULL,
SITUATION char (1)             ,
CPT       smallint     NOT NULL,
constraint PK_INGENIEUR primary key (MLE)
)



create table DEPT
(
CODEPT   int          NOT NULL,
DESIGN   char (20)    NOT NULL,
ADRESSE  varchar (30)         ,
CODEPOST char (5)             ,
VILLE    char  (30)           ,
NOTEL    char(14)             , 
PDG      char (20)            ,
DG  char     (20)             ,
constraint PK_DEPT primary key (CODEPT)
)


create table PROJET
(
CODPRO   int          NOT NULL,
LIBPRO   varchar (30) NOT NULL,
ADRESSE  varchar (30)         ,
CODEPOST char    (5)          ,
VILLE    char    (30)         ,
COUT     decimal (10,2)        ,
TYPE     smallint             ,
constraint PK_PROJET primary key (CODPRO),
constraint FK_PROJET
         foreign key (TYPE) references TYPE(TYPE)
         on delete no action
)




create table CLIENT
(
CODCLI   int         NOT NULL,
DESIGN   char (20)   NOT NULL,
NOTEL    char (14)           ,
ADRESSE  varchar (30)        ,
CODEPOST char (5)            ,
VILLE    char (30)           ,
CORRESP  char (20)           ,
LIBSECT  char (20)           ,
constraint PK_CLIENT primary key (CODCLI)
)

create table EMPLOI
(
MLE    int      NOT NULL,
DATENT datetime NOT NULL,
CODEPT int      NOT NULL,
DATSOR datetime         ,
constraint PK_EMPLOI primary key (MLE, DATENT),
constraint FK_EMPLOI_CODEPT
         foreign key (CODEPT) references DEPT (CODEPT)
         on delete no action,
constraint FK_EMPLOI_MLE
         foreign key (MLE) references INGENIEUR (MLE)
         on delete cascade
)
create table AFFECT
(
MLE  int       NOT NULL,
DATDEB datetime NOT NULL,
CODPRO  int       NOT NULL,
DATFIN  datetime,
FONCTION  char (20)    NOT NULL,
PVENTE  smallint  NOT NULL,
constraint PK_AFFECT primary key (MLE, DATDEB),
constraint FK_AFFECT_MLE
         foreign key (MLE) references INGENIEUR (MLE)
         on delete cascade,
constraint FK_AFFECT_CODPRO
         foreign key (CODPRO) references PROJET (CODPRO)
         on delete cascade
)
create table CLI_PRO
(
CODCLI int NOT NULL,
CODPRO int NOT NULL,
constraint PK_CLI_PRO primary key (CODCLI, CODPRO),
constraint FK_CLIPRO_CODCLI
         foreign key (CODCLI) references CLIENT(CODCLI)
         on delete no action,
constraint FK_CLIPRO_CODPRO
         foreign key (CODPRO) references PROJET(CODPRO)
         on delete no action
)
create table ING_CPC
(
MLE    int       NOT NULL,
COMPET char (20) NOT NULL ,
constraint PK_ING_CPC primary key (MLE, COMPET),
constraint FK_ING_CPC
         foreign key (MLE) references INGENIEUR (MLE)
         on delete cascade
)

create table PRO_CPC
(
CODPRO int       NOT NULL,
COMPET char (20) NOT NULL ,
constraint PK_PRO_CPC primary key (CODPRO, COMPET),
constraint FK_PRO_CPC
         foreign key (CODPRO) references PROJET(CODPRO)
         on delete cascade
)
create table ING_PRO_CPC
(
MLE    int       NOT NULL,
CODPRO int       NOT NULL,
COMPET char (20) NOT NULL ,
constraint PK_ING_PRO_CPC primary key (MLE, CODPRO, COMPET),
constraint FK_ING_PRO_CPC1
         foreign key (MLE, COMPET) references ING_CPC(MLE, COMPET)
         on delete cascade,
constraint FK_ING_PRO_CPC2
         foreign key (CODPRO, COMPET) references PRO_CPC(CODPRO, COMPET)
         on delete cascade
)

SET DATEFORMAT ymd

